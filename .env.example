# RegisterUZ API Configuration
REGISTERUZ_BASE_URL=https://www.registeruz.sk/cruz-public/api

# Optional: Request timeout in seconds (default: 30)
REGISTERUZ_TIMEOUT=30

# Optional: Maximum records per request (default: 1000, max: 10000)
REGISTERUZ_MAX_RECORDS=1000

# Optional: Default date for initial data fetch (format: YYYY-MM-DD)
REGISTERUZ_DEFAULT_FROM_DATE=2000-01-01

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO